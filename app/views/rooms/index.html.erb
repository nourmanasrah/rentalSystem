<div class="container room-view">
<div calss="row">
    <div class="col-md-3">
        <ul class="sidebar-list">
            <li class="sidebar-item"><%= link_to "Your Listing", rooms_path, class: "sidebar-link sidebar-lebal active"%></li>
            <li class="sidebar-item"><%= link_to "Your Reservations", rooms_path, class: "sidebar-link sidebar-lebal"%></li>
        </ul>
    </div>
    <div class="col-md-9">
        <div class="panel panel-default">
        <div class="panel-heading">
            Listing
        </div>
        <div class="panel-body">
          <% @rooms.each do |room|%>
            <div class="row">
              <div class="col-md-2">
                
              </div>
              <div class="col-md-7">
                  <h4><%= room.list_name %></h4>
              </div>
              <div class="col-md-3 right"><%= link_to "Update", edit_room_path(room), class: "btn btn-primary"%></div>
            </div>
          <% end %>
        </div> 
        </div>
    </div>
  </div>
</div>